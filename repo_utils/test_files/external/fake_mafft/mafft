#!/usr/bin/env python3
"""
Given a haps.fa, return the pre-computed result
"""
import os
import sys
import json
import hashlib

sys.stderr.write(f"Running fake MAFFT\n")
m_dir = os.path.dirname(os.path.realpath(__file__))

md5sum = hashlib.md5(sys.stdin.read().encode()).hexdigest()

with open(os.path.join(m_dir, f"lookup/fm_{md5sum}.msa")) as result:
    sys.stdout.write(result.read())
